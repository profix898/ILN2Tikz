<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.3.0</Version>
    <Copyright>Copyright © 2017-2020 Thilo Viereck</Copyright>
    <RepositoryUrl>https://github.com/profix898/ILN2Tikz</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Thilo Viereck</Authors>
    <Company />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Export functionality for ILNumerics (http://ilnumerics.net/) scene graphs and plot cubes to Tikz (LaTeX graphics package)</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ILNumerics_deploy\ILNHelper.cs" Condition="$(EnableDefaultCompileItems) != 'true'" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="EMG.External.ILNumerics" Version="5.5.7504" />
  </ItemGroup>
  <ItemGroup>
    <None Include="ILNumerics_deploy\ILNImports1.targets" />
    <None Include="ILNumerics_deploy\ILNImports2.targets" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ILNumerics_deploy\ilnumerics.lic" />
  </ItemGroup>
  <Import Project="ILNumerics_deploy\ILNImports1.targets" Condition="$(ILNLicCompile) != 'true'" />
  <Import Project="ILNumerics_deploy\ILNImports2.targets" Condition="$(ILNLicCompile) == 'true'" />
</Project>